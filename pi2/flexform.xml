<T3DataStructure>
	<meta type="array">
		<langDisable>1</langDisable>
	</meta>
	<sheets type="array">
		<sDEF type="array">
			<ROOT type="array">
				<TCEforms type="array">
					<sheetTitle>LLL:EXT:dam_frontend/pi2/locallang.xml:ffSheetSettings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el type="array">
					<subheader>
						<TCEforms>
							<label>LLL:EXT:dam_frontend/pi2/locallang.xml:ffOptionLabelSubheader</label>
							<config>
								<type>input</type>
								<size>30</size>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</subheader>
					<damUid>
						<TCEforms>
							<label>LLL:EXT:dam_frontend/pi2/locallang.xml:ffOptionLabelDamUids</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>tx_dam</allowed>
								<size>5</size>
								<maxitems>100</maxitems>
								<minitems>0</minitems>
								<autoSizeMax>10</autoSizeMax>
								<show_thumbs>0</show_thumbs>
								<wizards>
									<suggest>
										<type>suggest</type>
									</suggest>
								</wizards>
							</config>
						</TCEforms>
					</damUid>
					<templateFile>
						<TCEforms>
							<label>LLL:EXT:dam_frontend/pi2/locallang.xml:ffOptionLabelTemplateFile</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
								</items>
								<itemsProcFunc>tx_damfrontend_addfilestosel->main</itemsProcFunc>
							</config>
						</TCEforms>
					</templateFile>
				</el>
			</ROOT>
		</sDEF>
		<sLINKS>
			<ROOT>
				<TCEforms type="array">
					<sheetTitle>LLL:EXT:dam_frontend/pi2/locallang.xml:ffSheetLinks</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<links>
						<TCEforms>
							<label>LLL:EXT:dam_frontend/pi2/locallang.xml:ffOptionLabelLinks</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>5</size>
								<minitems>0</minitems>
								<maxitems>20</maxitems>
								<wizards type="array">
									<suggest>
										<type>suggest</type>
									</suggest>
								</wizards>
							</config>
						</TCEforms>
					</links>
					<linktext>
						<TCEforms>
							<label>LLL:EXT:dam_frontend/pi2/locallang.xml:ffOptionLabelLinkText</label>
							<config>
								<type>text</type>
								<rows>5</rows>
								<cols>34</cols>
							</config>
						</TCEforms>
					</linktext>
					<linkheader>
						<TCEforms>
							<label>LLL:EXT:dam_frontend/pi2/locallang.xml:ffOptionLabelLinkTitle</label>
							<config>
								<type>text</type>
								<rows>5</rows>
								<cols>34</cols>
							</config>
						</TCEforms>
					</linkheader>
					<linkdescription>
						<TCEforms>
							<label>LLL:EXT:dam_frontend/pi2/locallang.xml:ffOptionLabelLinkDescription</label>
							<config>
								<type>text</type>
								<rows>5</rows>
								<cols>34</cols>
							</config>
						</TCEforms>
					</linkdescription>
				</el>
			</ROOT>
		</sLINKS>
	</sheets>
</T3DataStructure>